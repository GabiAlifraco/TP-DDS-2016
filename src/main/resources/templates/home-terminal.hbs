{{# partial "titulo" }}	
<title>
  Home Terminal
</title>
{{/partial}}

{{#partial "header" }}
<link rel="stylesheet"  href="css/home-terminal.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js" ></script>
<script src="/js/terminal.js"></script>
{{/partial}}


<body class="body-terminal">
{{#partial "contenido" }}
<div>
<h1 align="center" style="padding-top:3%"><strong> Buscar Punto de Interés</strong><br></h1>
</div><br>

<div id="tipos" class="row shortcut">

        <a href="/terminal/bancos" style="cursor: auto" id="banco" class="col-xs-12 col-md-3 shortcut"  data-original-title="" title="">
          <span class="banco">
            <img alt="banco" src="images/banco.png" style="width:60px;height:60px;">
          </span>
          <h3>Banco</h3>
        </a>
        
        <a href="/terminal/cgps" style="cursor: auto" class="col-xs-12 col-md-3 shortcut"  data-original-title="" title="">
          <span class="CGP">
          <img alt="Centro de gestion y participacion" src="images/cgp.png" style="width:60px;height:60px;">
          </span>
          <h3>Centro de Gestión y Participación</h3>
        </a>
        
        <a href="/terminal/paradas" style="cursor: auto" class="col-xs-12 col-md-3 shortcut"  data-original-title="" title="">
          <span class="Colectivo">
            <img alt="Parada de Colectivo" src="images/bus.ico" style="width:60px;height:60px;">
          </span>
          <h3>Colectivo</h3>
        </a>
        
        <a href="/terminal/locales" style="cursor: auto" class="col-xs-12 col-md-3 shortcut"  data-original-title="" title="">
          <span class="LocalComercial">
            <img alt="Local Comercial" src="images/13702-200.png" style="width:60px;height:60px;">
          </span>
          <h3>Comercio</h3>
		</a>
		
</div>
      
{{/partial}}
</body>

